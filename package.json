{
  "name": "pub-bot",
  "version": "1.0.0",
  "description": "\"pub suggestion slack bot\"",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "prestart": "tsc",
    "start": "node dist/index.js",
    "predebug": "tsc",
    "debug": "node --debug dist/index.js",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@slack/client": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "20.0.2",
    "@types/node": "20.17.0",
    "jest": "20.0.4",
    "typescript": "2.3.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/test/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/*.spec.ts"
    ]
  }
}
